<%= simple_form_for(@list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :due_date %>
    <%= f.hidden_field :user_id , value: current_user.id %>
    <%= f.hidden_field :category_id, value: 1 %>
  </div>



  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>





